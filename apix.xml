<ROSETTASCRIPTS>
    <TASKOPERATIONS>
    </TASKOPERATIONS>
    <SCOREFXNS>
        <dens weights=talaris2014 symmetric=1>
            <Reweight scoretype=cart_bonded_length weight=0.5/>
            <Reweight scoretype=cart_bonded_torsion weight=0.5/>
            <Reweight scoretype=cart_bonded_angle weight=1.0/>
            <Reweight scoretype=pro_close weight=0.0/>
            Reweight scoretype=fa_sol weight=0.0/>
            <Reweight scoretype=elec_dens_fast weight="%%fa_wt%%"/>
	    <Reweight scoretype=rama weight=0.0/>
            <Reweight scoretype=rama_prepro weight=1.0/>	
            <Set scale_sc_dens_byres="E:0.56,D:0.56,R:0.76,K:0.76,M:0.76,C:0.81,Q:0.81,H:0.81,N:0.81,T:0.81,S:0.81,Y:0.88,W:0.88,A:0.88,F:0.88,P:0.88,I:0.88,L:0.88,V:0.88"/>
        </dens>
    </SCOREFXNS>
    <MOVERS>
        <SetupForSymmetry name=setupsymm definition="./symm_def.txt" />
        <LoadDensityMap name=loaddens mapfile="./map.mrc"/>
        <ReportFSC name=report res_high="%%res%%" res_low=10.0/>
        <SetupForDensityScoring name=setupdens realign=min/>
        <VoxelSpacingRefinement name="apix" aniso=0 mapout="apix_refined.mrc"/>
    </MOVERS>
    <PROTOCOLS>
        <Add mover=setupsymm/>
        <Add mover=loaddens/>
        <Add mover=apix/>
        <Add mover=report/>
    </PROTOCOLS>
    <OUTPUT scorefxn=dens/>
</ROSETTASCRIPTS>
